;-------------------------------------------------------------------------------
;
[ssud.Install.nt]
Characteristics = 0x84 ; NCF_PHYSICAL,NCF_HAS_UI 
BusType         = 15 ; if you specify NCF_PHYSICAL, you must specify bustype
AddReg          = ssud.Reg, ParamsPromiscuous
CopyFiles       = ssud.CopyFiles

[ssud.Install.NT.Services]
AddService      = ssudnd5, 2, ssud.Service, ssud.EventLog


;-----------------------------------------------------------------------------
;
[ssud.Reg]
HKR,    ,                         BusNumber,           0, "0"
HKR, Ndi,                         Service,             0, "ssudnd5"
HKR, Ndi\Interfaces,              UpperRange,          0, "ndis5"
HKR, Ndi\Interfaces,              LowerRange,          0, "ethernet"

[ParamsPromiscuous]
;
;    Should the physical NIC be set to Promiscuous mode
;
HKR, Ndi\Params\Promiscuous,     ParamDesc, , %Promiscuous%
HKR, Ndi\Params\Promiscuous,     Default,  ,"0"
HKR, Ndi\Params\Promiscuous,     type, ,      enum
HKR, Ndi\Params\Promiscuous\enum,"1",  ,     %Promiscuous_Enable%
HKR, Ndi\Params\Promiscuous\enum,"0",  ,     %Promiscuous_Disable%


;-----------------------------------------------------------------------------
; DestinationDirs
;
[DestinationDirs]
ssud.CopyFiles = 12

[ssud.CopyFiles]
ssudnd5.sys,,,2

;-----------------------------------------------------------------------------
; Driver and Service Section
;

[ssud.Service]
DisplayName     = %ssud.Service.DispName%
Description	= %ssud.Service.Desc%
ServiceType     = 1 ;%SERVICE_KERNEL_DRIVER%
StartType       = 3 ;%SERVICE_DEMAND_START%
ErrorControl    = 1 ;%SERVICE_ERROR_NORMAL%
ServiceBinary   = %12%\ssudnd5.sys
LoadOrderGroup  = NDIS
AddReg          = TextModeFlags.Reg

[ssud.EventLog]
AddReg = ssud.AddEventLog.Reg

[ssud.AddEventLog.Reg]
HKR, , EventMessageFile, 0x00020000, "%%SystemRoot%%\System32\netevent.dll"
HKR, , TypesSupported,   0x00010001, 7

[TextModeFlags.Reg]
HKR, , TextModeFlags,    0x00010001, 0x0001

[SourceDisksNames.x86]
1 = %DiskId1%,,,\i386

[SourceDisksNames.amd64]
1 = %DiskId1%,,,\amd64

[SourceDisksFiles]
ssudnd5.sys  = 1,,

;
;--- ssud.Install WDF Coinstaller installation ------
;
[DestinationDirs]
ssud_CoInstaller_CopyFiles = 11

[ssud.Install.NT.CoInstallers]
AddReg=ssud_CoInstaller_AddReg
CopyFiles=ssud_CoInstaller_CopyFiles

[ssud_CoInstaller_AddReg]
HKR,,CoInstallers32,0x00010000, "WdfCoInstaller01007.dll,WdfCoInstaller"
;HKR,,CoInstallers32,0x00010000, "WdfCoInstaller01009.dll,WdfCoInstaller"

[ssud_CoInstaller_CopyFiles]
WdfCoInstaller01007.dll
;WdfCoInstaller01009.dll

[SourceDisksFiles]
WdfCoInstaller01007.dll=1 ; make sure the number matches with SourceDisksNames
;WdfCoInstaller01009.dll=1

[ssud.Install.NT.Wdf]
KmdfService = ssudnd5, ssud_wdfsect
[ssud_wdfsect]
KmdfLibraryVersion = 1.7
;KmdfLibraryVersion = 1.9



